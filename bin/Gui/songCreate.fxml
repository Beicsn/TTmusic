<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="153.0" prefWidth="314.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.songCreateController">
    <VBox alignment="CENTER">
        <TextField fx:id="newSongNameText" prefHeight="34.0" prefWidth="314.0" promptText="新的歌曲文件名" />
      <Label fx:id="filePlace">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
        <Button fx:id="createNewSongButton" disable="true" mnemonicParsing="false" onAction="#buttonCreateSong" styleClass="button_songMenu" text="创建">
            <VBox.margin>
                <Insets top="20.0" />
            </VBox.margin></Button>


    </VBox>
</AnchorPane>
